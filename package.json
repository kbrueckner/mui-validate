{
  "name": "mui-validate",
  "version": "0.1.0",
  "description": "Validation tools for Material UI components and component groups",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "scripts": {
    "build": "rollup -c && rm -r dist/dts",
    "build-watch": "rollup -c -w",
    "test:watch": "jest --watchAll",
    "test:unit": "jest",
    "test:e2e": "JEST_PLAYWRIGHT_CONFIG=e2e-tests/config/playwright.json jest --config jest.config.e2e.json",
    "lint": "eslint . --cache --ext .ts",
    "start-playground": "cd playground && npm run start",
    "i-all": "npm i && cd playground && npm i",
    "dev": "npm-run-all --parallel build-watch start-playground",
    "release": "npm run lint && npm run test:unit && npm run build && node scripts/release.js"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x || 17.x"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^26.6.0",
    "jest-html-reporters": "^2.1.6",
    "jest-playwright-preset": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.16.2",
    "rollup": "^2.58.3",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "test-locator": "^0.2.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "validation",
    "validate",
    "mui",
    "material-ui",
    "@material-ui/core",
    "@mui/material"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbrueckner/mui-validate"
  },
  "author": "Kenji Br√ºckner <kenji.brueckner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kbrueckner/mui-validate/issues"
  },
  "homepage": "https://github.com/kbrueckner/mui-validate"
}
